<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Menu</title>
</head>
<body>
  <h1>Full Menu</h1>
  <% menu.forEach(function(item){ %>
<div>
    <h3><%= item.name %> - $<%= item.price.toFixed(2) %></h3>
    <p><%= item.details %></p>
    <p>Rating: <%= item.rating %>/5</p>
</div>
<hr>
<% }) %>
</body>
</html>
